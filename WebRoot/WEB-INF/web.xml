  	<servlet-class>action.PublicServiceAction</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowTrack</servlet-name>
  	<servlet-class>action.ShowTrack</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowMyTrack</servlet-name>
  	<servlet-class>action.ShowMyTrack</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteTrack</servlet-name>
    <servlet-class>action.DeleteTrack</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchMyTrack</servlet-name>
    <servlet-class>action.SearchMyTrack</servlet-class>
  </servlet>
	<servlet>
    <servlet-name>NoteAction</servlet-name>
    <servlet-class>action.NoteAction</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>LoginAction</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DetailAction</servlet-name>
    <url-pattern>/detail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegistAction</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegistGuideAction</servlet-name>
  	<url-pattern>/registguide</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PublicServiceAction</servlet-name>
  	<url-pattern>/publicService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServiceAction</servlet-name>
  	<url-pattern>/service</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SearchTrack</servlet-name>
  	<url-pattern>/searchtrack</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateUserInfo</servlet-name>
  	<url-pattern>/updateuserinfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreateTrack</servlet-name>
  	<url-pattern>/createtrack</url-pattern>
