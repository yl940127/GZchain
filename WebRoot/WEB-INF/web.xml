    <url-pattern>/showtrack</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteTrack</servlet-name>
    <url-pattern>/deletetrack</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchMyTrack</servlet-name>
    <url-pattern>/searchmytrack</url-pattern>
  </servlet-mapping>

	<servlet-mapping>
    <servlet-name>NoteAction</servlet-name>
    <url-pattern>/note</url-pattern>
  </servlet-mapping>	

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
