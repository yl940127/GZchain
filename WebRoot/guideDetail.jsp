			function jump(id,name){
				if(!id||!name){
					return false;
				}
				var list = {}, num = 1;
				list[id] = name;
				for (var i in search) {
					if (search[i] != name) {
						list[i] = search[i];
						num++;
					}
					if (num >= 10) {
						break;
					}
				}
				localStorage.setItem('search', JSON.stringify(list));
				window.location.href = '/site/list/id/' + id;
			}
