<script type="text/javascript">
    //游记统计
    $(function(){
        var enterTime = new Date().getTime(),
            offsetPercent = 0;
        $(window).on('scroll',function(){
            var waypoints = $('.waypoint'),
                scrollTop = document.body.scrollTop;
            waypoints.each(function(index,item){
                if($(item).position().top >= scrollTop){
                    offsetPercent = Math.max((index+1)/waypoints.length,offsetPercent);
                    return false;
                }
            });
        });
        $(window).on('beforeunload',function(){
            //游记停留时间统计
            bt.log.log({
                module: 'trip',
                page: 'trip',
                pageValue : $('#trip_id').attr('value'),
                data : "1001:" + ( new Date().getTime() - enterTime )
            });
            //游记最长查看百分比统计
            bt.log.log({
                module: 'trip',
                page: 'trip',
                pageValue : $('#trip_id').attr('value'),
                data : "2001:" + offsetPercent.toFixed(4)
            });
        });
    });

</script>


